<?xml version="1.0" encoding="utf-8"?>
<Domain id="conditionList" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" >
    <Attribute id="conditions" name="Conditions">
        <List>
            <Domain>
                <Attribute id="name" name="Name">
                    <Str default="(none)" />
                </Attribute>
                <Attribute id="value" name="Value">
                    <Bool />
                </Attribute>
                <Attribute id="note" name="Note">
                    <Text />
                </Attribute>
            </Domain>
        </List>
    </Attribute>
</Domain>
